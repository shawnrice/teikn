{
  "name": "teikn",
  "version": "1.0.0-alpha.5",
  "description": "Generate design tokens",
  "keywords": [
    "design",
    "design tokens",
    "design-systems"
  ],
  "homepage": "https://github.com/shawnrice/teikn#readme",
  "bugs": {
    "url": "https://github.com/shawnrice/teikn/issues"
  },
  "license": "MIT",
  "author": "Shawn Patrick Rice <rice@shawnrice.org>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "teikn": "./lib/src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shawnrice/teikn.git"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write 'src/**/*.{json,ts,md}' './*.{js|json}'",
    "lint": "eslint --ext .ts -c .eslintrc.js './src/**/*.ts'",
    "prepublishOnly": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.4",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.157",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.0.19",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "mixin-deep": "^1.3.2",
    "handlebars": "^4.5.1",
    "set-value": "^2.0.1"
  }
}

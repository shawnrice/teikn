{
  "name": "teikn",
  "version": "1.0.0-alpha.4",
  "description": "Generate design tokens",
  "keywords": [
    "design",
    "design tokens",
    "design-systems"
  ],
  "homepage": "https://github.com/shawnrice/teikn#readme",
  "bugs": {
    "url": "https://github.com/shawnrice/teikn/issues"
  },
  "license": "MIT",
  "author": "Shawn Patrick Rice <rice@shawnrice.org>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "teikn": "./lib/src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shawnrice/teikn.git"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write 'src/**/*.{json,ts,md}' './*.{js|json}'",
    "lint": "eslint --ext .ts -c .eslintrc.js './src/**/*.ts'",
    "prepublishOnly": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.12.11",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "mixin-deep": "^1.3.2",
    "handlebars": "^4.5.1",
    "set-value": "^2.0.1"
  }
}

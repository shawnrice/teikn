{
  "name": "teikn",
  "version": "1.0.0-alpha.6",
  "description": "Generate design tokens",
  "keywords": [
    "design",
    "design tokens",
    "design-systems"
  ],
  "homepage": "https://github.com/shawnrice/teikn#readme",
  "bugs": {
    "url": "https://github.com/shawnrice/teikn/issues"
  },
  "license": "MIT",
  "author": "Shawn Patrick Rice <rice@shawnrice.org>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "teikn": "./lib/src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shawnrice/teikn.git"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write 'src/**/*.{json,ts,md}' './*.{js|json}'",
    "lint": "eslint --ext .ts './src/**/*.ts'",
    "prepublishOnly": "tsc",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "tslib": "^2.0.0",
    "ktree": "4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.5",
    "@babel/plugin-transform-modules-commonjs": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.0.3",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^27.4.5",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}

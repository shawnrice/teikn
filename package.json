  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "color": "^3.1.2",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@types/color": "^3.0.0",
    "@types/lodash": "^4.14.135",
    "@types/node": "^12.0.10",
    "babel-jest": "^24.8.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "husky": "^2.7.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "jest": {
    "cacheDirectory": "<rootDir>/.jest-cache",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**"
    ],
    "coverageDirectory": ".coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/out/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "verbose": true
  }

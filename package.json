{
  "name": "teikn",
  "version": "1.0.0-alpha.6",
  "description": "Generate design tokens",
  "keywords": [
    "design",
    "design tokens",
    "design-systems"
  ],
  "homepage": "https://github.com/shawnrice/teikn#readme",
  "bugs": {
    "url": "https://github.com/shawnrice/teikn/issues"
  },
  "license": "MIT",
  "author": "Shawn Patrick Rice <rice@shawnrice.org>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "teikn": "./lib/src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shawnrice/teikn.git"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write 'src/**/*.{json,ts,md}' './*.{js|json}'",
    "lint": "eslint --ext .ts './src/**/*.ts'",
    "prepublishOnly": "tsc",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "lodash": "^4.0.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.33",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
